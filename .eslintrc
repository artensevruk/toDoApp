{
  "env": { "es6": true },
  "parserOptions": { "ecmaVersion": 9, "sourceType": "module" },

  "rules": {
    "no-debugger": 2,
    "indent": [2, 2],
    "no-trailing-spaces": 2,
    "eqeqeq": 2,
    "semi": [2, "always"],
    "quotes": [2, "double"],
    "prefer-const": 2,
    "no-unused-vars": ["error", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }]
  }
  
}
